<%= form_for(:food, url: user_foods_path) do |f|%>
<%= f.label :food_name %>
<%= f.text_field :food_name, :placeholder => "Food Name" %>
<%= f.label :quantity %>
<%= f.text_field :quantity, :placeholder => "Quantity" %>
<%= f.label :expiration %>
<%= f.date_field :expiration, :placeholder => "Expiration" %>
<%= f.submit "Submit" %>
<% end %>